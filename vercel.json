{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/style.css",
      "dest": "/public/style.css"
    },
        {
      "src": "/icon.gif",
      "dest": "/public/icon.gif"
    },
        {
      "src": "/icon_static.png",
      "dest": "/public/icon_static.png"
    },
    {
      "src": "/(.*\\.(?:css|js|png|jpg|jpeg|gif|svg|ico|webp|woff2?|ttf|eot))",
      "dest": "/public/$1"
    }
  ],
  "regions": ["bom1"]
}
